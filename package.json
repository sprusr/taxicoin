{
  "name": "taxicoin",
  "version": "1.0.0",
  "description": "Decentralised ride sharing app",
  "author": "sprusr <scott@spru.sr>",
  "private": true,
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "npm-run-all -p testrpc contract dev",
    "build": "node build/build.js",
    "testrpc": "ganache-cli -p 7545",
    "contract": "npm-run-all contract-compile contract-migrate contract-sync-dist",
    "contract-migrate": "truffle migrate",
    "contract-compile": "truffle compile",
    "contract-sync-dist": "rimraf dist/contracts && cp -R build/contracts/ dist/contracts",
    "contract-test": "truffle test",
    "int": "cross-env BABEL_ENV=test karma start test/int/karma.conf.js --single-run",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run contract-test && npm run int && npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.3",
